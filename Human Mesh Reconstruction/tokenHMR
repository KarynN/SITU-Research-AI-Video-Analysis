rson in the frame\n","    for person_number, person_smpl_data in enumerate(info['smpl']):\n","        smpl_data = info['smpl'][person_number]\n","        camera_data = info['camera'][person_number]\n","        frame_path = info['frame_path']  # Assuming frame_path is in the same dictionary level\n","\n","        # Extract pose, global orient, and betas\n","        body_pose_matrix = smpl_data['body_pose'] # Shape: (23, 3, 3)\n","        global_orient_matrix = smpl_data['global_orient']  # Shape: (3, 3)\n","        betas = smpl_data['betas'] # Shape: (10,)\n","\n","        # Convert body_pose (23, 3, 3) to axis-angle (1, 69)\n","        pose_rotations = R.from_matrix(body_pose_matrix)\n","        pose_axis_angle = pose_rotations.as_rotvec().reshape(-1)  # Flatten to (69,)\n","        pose_axis_angle = torch.tensor(pose_axis_angle, dtype=torch.float32).unsqueeze(0)  # (1, 69)\n","\n","        # Convert global_orient (3, 3) to axis-angle (1, 3)\n","        global_orient_rotation = R.from_matrix(global_orient_matrix)\n","        global_orient_axis_angle = global_orient_rotation.as_rotvec()\n","        global_orient_axis_angle = torch.tensor(global_orient_axis_angle, dtype=torch.float32)  # (1, 3)\n","\n","        # Convert betas to tensor and add batch dimension (1, 10)\n","        betas = torch.tensor(betas, dtype=torch.float32).unsqueeze(0)  # (1, 10)\n","\n","        # Set the transl (translation) vector to move each person in the frame 1 unit further along the Z-axis\n","        transl = torch.tensor([[0.0, 0.0, person_number]], dtype=torch.float32)  # Translation: Z = person_number units\n","\n","        # Pass the parameters into the SMPL model with the custom translation\n","        output = smpl.forward(\n","            betas=betas,  # (1, 10)\n","            body_pose=pose_axis_angle,  # (1, 69)\n","            global_orient=global_orient_axis_angle,  # (1, 3)\n","            transl=transl  # Apply translation to move each person along Z-axis\n","        )\n","\n","        # Extract vertices (mesh)\n","        vertices = output.vertices.detach().cpu().numpy()  # Shape: (1, 6890, 3)\n","\n","        # Append the vertices and camera data for this person, along with the translation vector\n","        frame_vertices.append([frame_path, vertices[0], camera_data])  # Remove batch dimension, shape: (6890, 3)\n","\n","    # Append all persons' vertices, camera, and transl for this frame\n","    vertices_list.append(frame_vertices)\n","\n","# `vertices_list` now has the structure: frame -> person -> [frame_path, vertices, camera, transl]\n","print(\"Vertices generated, stored with camera and translation in nested list\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"SbhEKZmgN_0F","executionInfo":{"status":"ok","timestamp":1726699405600,"user_tz":240,"elapsed":13762,"user":{"displayName":"Karyn Nakamura","userId":"07789206291232155731"}},"outputId":"79cfab8e-6242-42fd-89aa-fcb70a34b156"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["WARNING: You are using a SMPL model, with only 10 shape coefficients.\n","Vertices generated, stored with camera and translation in nested list\n"]}]},{"cell_type":"code","source":["vertices_list[0][1][1]"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"q3yCnrB3YRqT","executionInfo":{"status":"ok","timestamp":1726507538906,"user_tz":240,"elapsed":405,"user":{"displayName":"Karyn Nakamura","userId":"07789206291232155731"}},"outputId":"495ce38b-8226-49c5-9d5a-2e22c01e7f47"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["array([[-0.06425676, -0.90447855,  1.225932  ],\n","       [-0.05867215, -0.8894149 ,  1.2295114 ],\n","       [-0.06901298, -0.8880398 ,  1.2191979 ],\n","       ...,\n","       [ 0.06187621, -0.8752004 ,  1.1298931 ],\n","       [ 0.06195341, -0.87681675,  1.1318295 ],\n","       [ 0.06459308, -0.8739667 ,  1.1340451 ]], dtype=float32)"]},"metadata":{},"execution_count"